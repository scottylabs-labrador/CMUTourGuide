{
  "expo": {
    "name": "TourGuide",
    "slug": "TourGuide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/appstore.png",
    "scheme": "tourguide",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bigboa19.TourGuide",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSLocationWhenInUseUsageDescription": "CMUTourGuide uses your location to show your position on the campus map."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyA2UL82O5NP-hY4x1WeoOKbKqiXq1GEUko"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION"
      ],
      "package": "com.bigboa19.TourGuide"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow CMUTourGuide to access your camera"
        }
      ],
      [
        "expo-dev-client",
        {
          "addGeneratedScheme": false
        }
      ],
      [
        "expo-location",
        {
          "locationWhenInUsePermission": "CMUTourGuide uses your location to show your position on the campus map."
        }
      ],
      "expo-font"
    ],
    "developmentClient": {
      "silentLaunch": true
    },
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "6ed9be8d-fbc9-4812-b934-99b2e359f21b"
      }
    }
  }
}
